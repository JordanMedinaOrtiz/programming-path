Algoritmo Ciclo_Anidados_Figura
	
	Definir i,j Como entero;
	Definir figura como caracter;
	
	Leer figura;
	
	Para i=5 hasta 1 Con Paso -1
		
		Para j=i hasta 5
			Escribir figura Sin Saltar;
		FinPara
		
		Escribir "";
	FinPara
	
	Para i=1 hasta 4 
		
		Para j=i hasta 4
			Escribir figura Sin Saltar;
		FinPara
		
		Escribir "";
	FinPara
	
FinAlgoritmo